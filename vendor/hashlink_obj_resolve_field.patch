 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/src/std/obj.c b/src/std/obj.c
index f90854f..0be8c8f 100644
--- a/src/std/obj.c
+++ b/src/std/obj.c
@@ -61,7 +61,8 @@ static int hl_lookup_find_index( hl_field_lookup *l, int size, int hash ) {
 	return (min + max) >> 1;
 }
 
-static hl_field_lookup *obj_resolve_field( hl_type_obj *o, int hfield ) {
+/* Made non-static for HLFFI static field/method access */
+hl_field_lookup *obj_resolve_field( hl_type_obj *o, int hfield ) {
 	hl_runtime_obj *rt = o->rt;
 	do {
 		hl_field_lookup *f = hl_lookup_find(rt->lookup,rt->nlookup,hfield);
